{
  "compilerOptions": {
    "lib": [
      "ES2022",
      "DOM",
      "DOM.Iterable"
    ],
    "module": "ES2022",
    "target": "ES2022",
    "moduleResolution": "bundler",
    "isolatedModules": true,
    "strict": true,
    "esModuleInterop": true,
    "declaration": false,
    "outDir": "./dist",
    "jsx": "react-jsx",
    "skipLibCheck": true
  },
  "include": [
    "src"
  ]
}
